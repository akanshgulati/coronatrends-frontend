{
  "author": "Akansh",
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "24": "icon-32.png",
      "32": "icon-32.png"
    },
    "default_title": "COVID-19 Tracker",
    "default_popup": "index.html"
  },
  "description": "A utility to provide updates on Covid-19 coronavirus cases",
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png",
    "48": "icon-64.png"
  },
  "manifest_version": 2,
  "name": "Covid-19 Tracker",
  "permissions": [
    "alarms",
    "storage",
    "https://api.coronatrends.live/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "version": "1.0.0"
}